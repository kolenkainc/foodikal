{
  "name": "foodikal-api",
  "description": "A template for building a React application with Vite, Hono, and Cloudflare Workers",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "lint:staged": "lint-staged",
    "backend:types": "wrangler types",
    "dev": "wrangler dev --live-reload",
    "build": "npm run backend:types && tsc --build",
    "deploy": "wrangler deploy",
    "test": "vitest run --coverage",
    "db:generate": "drizzle-kit generate --config=src/db/drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config=src/db/drizzle.config.ts",
    "db:seed": "tsx ./src/db/seed.ts"
  },
  "cloudflare": {
    "label": "Vite React Template",
    "products": [
      "Workers"
    ],
    "categories": [],
    "icon_urls": [
      "https://imagedelivery.net/wSMYJvS3Xw-n339CbDyDIA/5ca0ca32-e897-4699-d4c1-6b680512f000/public"
    ],
    "preview_image_url": "https://imagedelivery.net/wSMYJvS3Xw-n339CbDyDIA/fc7b4b62-442b-4769-641b-ad4422d74300/public",
    "dash": true
  },
  "dependencies": {
    "@hono/sentry": "^1.2.1",
    "@hono/swagger-ui": "^0.5.1",
    "@neondatabase/serverless": "^1.0.1",
    "@sentry/cloudflare": "^9.17.0",
    "@sentry/vite-plugin": "^3.2.4",
    "drizzle-orm": "^0.44.3",
    "google-spreadsheet": "^4.1.4",
    "googleapis": "^148.0.0",
    "hono": "4.7.1",
    "postgres": "^3.4.5",
    "tsyringe": "^4.10.0"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^0.1.19",
    "@cloudflare/workers-types": "^4.20250327.0",
    "@codecov/vite-plugin": "^1.9.1",
    "@eslint/js": "9.19.0",
    "@types/node": "^22.13.17",
    "@vitest/coverage-v8": "^3.1.2",
    "dotenv": "^16.5.0",
    "drizzle-kit": "^0.31.4",
    "eslint": "9.19.0",
    "globals": "15.14.0",
    "jsdom": "^26.1.0",
    "kolenkainc-honohub": "^0.4.1",
    "lint-staged": "^16.0.0",
    "nolyfill": "^1.0.44",
    "pg-connection-string": "^2.8.5",
    "postgres-shift": "^0.1.0",
    "prettier": "^3.5.3",
    "reflect-metadata": "^0.2.2",
    "tsx": "^4.20.3",
    "typescript": "5.7.2",
    "typescript-eslint": "8.22.0",
    "vite": "6.1.0",
    "vitest": "^3.1.2",
    "wrangler": "^4.25.0"
  },
  "overrides": {
    "es-set-tostringtag": "npm:@nolyfill/es-set-tostringtag@^1",
    "safe-buffer": "npm:@nolyfill/safe-buffer@^1",
    "safer-buffer": "npm:@nolyfill/safer-buffer@^1",
    "side-channel": "npm:@nolyfill/side-channel@^1"
  },
  "engines": {
    "npm": ">=10.0.0",
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "*.ts": "eslint --cache \"src/**/*.ts\""
  }
}
